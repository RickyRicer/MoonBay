  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/theresaqueryforthat/MoonBay/issues"
  },
  "homepage": "https://github.com/theresaqueryforthat/MoonBay#readme",
  "dependencies": {
    "apollo-server-express": "^3.6.7",
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "react-spinners": "^0.11.0"
  }
}
